<footer class="site-footer" itemscope itemtype="https://schema.org/WPFooter">
    <div class="layout-container flex-container-justified">
        <div class="layout-2col">
            <p>
                <small>
                    Copyright
                    <span itemscope itemtype="https://schema.org/<txp:variable name="--site-org-type" />">
                        <span itemprop="name">Organisation Name</span>.
                    </span>
                    <a href="<txp:site_url />contact">Contact us</a>.
                    <a href="<txp:site_url />privacy">Privacy</a>.
                </small>
            </p>
        </div>
        <txp:evaluate>
            <div class="layout-2col">
                <ul class="social-channels<txp:if_variable name="--social-media-icon-style" not value=""> <txp:variable name="--social-media-icon-style" /></txp:if_variable>">
                    <txp:if_variable name="--url-facebook" not value=""><li><a rel="external" href="<txp:variable name="--url-facebook" />" class="facebook">Facebook</a></li></txp:if_variable>
                    <txp:if_variable name="--url-github" not value=""><li><a rel="external" href="<txp:variable name="--url-github" />" class="github">GitHub</a></li></txp:if_variable>
                    <txp:if_variable name="--url-instagram" not value=""><li><a rel="external" href="<txp:variable name="--url-instagram" />" class="instagram">Instagram</a></li></txp:if_variable>
                    <txp:if_variable name="--url-linkedin" not value=""><li><a rel="external" href="<txp:variable name="--url-linkedin" />" class="linkedin">LinkedIn</a></li></txp:if_variable>
                    <txp:if_variable name="--url-pinterest" not value=""><li><a rel="external" href="<txp:variable name="--url-pinterest" />" class="pinterest">Pinterest</a></li></txp:if_variable>
                    <txp:if_variable name="--url-twitter" not value=""><li><a rel="external" href="<txp:variable name="--url-twitter" />" class="twitter">Twitter</a></li></txp:if_variable>
                    <txp:if_variable name="--url-youtube" not value=""><li><a rel="external" href="<txp:variable name="--url-youtube" />" class="youtube">YouTube</a></li></txp:if_variable>
                </ul>
            </div>
        </txp:evaluate>
    </div>
</footer>
<script async src="<txp:page_url type="theme_path" />/assets/js/app.js"></script>
<noscript><txp:text item="noscript" /></noscript>
